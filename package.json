{
  "name": "advantage_plus",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "ng build --base-href /webtrader/",
    "build-prod": "ng build --prod --base-href /webtrader/",
    "build-prod-forex": "ng build advantage_plus_forex --prod",
    "build-prod-fxflat": "ng build advantage_plus_fxflat --prod",
    "build-prod-ii": "ng build advantage_plus_ii --prod",
    "build-prod-tradefair": "ng build advantage_plus_tradefair --prod",
    "build-prod-whs": "ng build advantage_plus_whs --prod",
    "ng": "./node_modules/@angular/cli/bin/ng",
    "start-alone": "ng run",
    "start": "ng serve",
    "start-prod": "ng serve --prod",
    "stop": "for /f \"tokens=5\" %a in ('netstat -ano ^| find \"4200\" ^| find \"LISTENING\"') do taskkill /f /pid %a",
    "test": "ng test  advantage_plus --source-map=false",
    "test-forex": "ng test advantage_plus_forex --source-map=false",
    "test-fxflat": "ng test advantage_plus_fxflat --source-map=false",
    "test-ii": "ng test advantage_plus_ii --source-map=false",
    "test-tradefair": "ng test advantage_plus_tradefair --source-map=false",
    "test-whs": "ng test advantage_plus_whs --source-map=false",
    "test-coverage": "ng test advantage_plus --code-coverage --watch=false",
    "test-coverage-forex": "ng test advantage_plus_forex --code-coverage --watch=false",
    "test-coverage-fxflat": "ng test advantage_plus_fxflat --code-coverage --watch=false",
    "test-coverage-ii": "ng test advantage_plus_ii --code-coverage --watch=false",
    "test-coverage-tradefair": "ng test advantage_plus_tradefair --code-coverage --watch=false",
    "test-coverage-whs": "ng test advantage_plus_whs --code-coverage --watch=false",
    "lint": "ng lint advantage_plus",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json",
    "wd-update": "node_modules\\.bin\\webdriver-manager update --ie64",
    "wd-start": "node_modules\\.bin\\webdriver-manager start",
    "wd-stop": "for /f \"tokens=5\" %a in ('netstat -ano ^| find \"4444\" ^| find \"LISTENING\"') do taskkill /f /pid %a",
    "protractor": "node_modules\\.bin\\protractor ui-tests/t-dist/configs/protractor-cucumber-conf.js",
    "tsc-test": "node_modules\\.bin\\tsc -p ui-tests/tsconfig.ui.json",
    "ui-cucumber": "npm run tsc-test && npm run protractor",
    "tsc": "tsc"
  },
